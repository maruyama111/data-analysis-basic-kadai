      ],
      "source": [
        "import numpy as np\n",
        "A=np.array([[0,1],[2,3],[4,5]])\n",
        "B=np.array([[0,1,2,3],[4,5,6,7]])\n",
        "C=np.dot(A,B)\n",
        "D=C.max()\n",
        "print(f\"行列の積の計算結果は\\n{C}\\nです。\")\n",
        "print(f\"行列の積の最大値は{D}です。\")"
      ]
    }
  ]
